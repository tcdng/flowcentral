<?xml version="1.0"?>
<!-- Workflow application -->
<application name="workflow"
	description="$m{workflow.short.description}" label="$m{workflow.label}"
	module="workflow">
	<!-- Widget Types -->
	<widgetTypes>
		<widgetType name="wfchanneldestinationlist"
			description="$m{workflow.widgettype.wfchanneldestinationlist}"
			dataType="string" inputType="string"
			editor="!ui-select list:wfchanneldestinationlist listParams:$l{direction entity} blankOption:$m{blank.none}"
			renderer="!ui-label" />
		<widgetType name="wfsteppolicylist"
			description="$m{workflow.widgettype.wfsteppolicylist}" dataType="string"
			inputType="string"
			editor="!ui-select list:wfsteppolicylist listParams:$l{type} blankOption:$m{blank.none}"
			renderer="!ui-label" />
		<widgetType name="wfsteprulelist"
			description="$m{workflow.widgettype.wfsteprulelist}" dataType="string"
			inputType="string"
			editor="!ui-select list:rulelist listParams:$l{policy} blankOption:$m{blank.none}"
			renderer="!ui-label" />
	</widgetTypes>
	<!-- Applets -->
	<applets>
		<applet name="manageWorkflow"
			description="$m{workflow.workflow.manage}"
			label="$m{workflow.workflow.manage.label}" icon="project-diagram"
			type="manage_entitylist" entity="workflow" menuAccess="true">
			<property name="searchTable" value="workflowTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="true" />
			<property name="maintainForm" value="workflowForm" />
		</applet>
		<applet name="manageInteractiveSteps"
			description="$m{workflow.step.interactive.manage}"
			label="$m{workflow.step.interactive.manage.label}" type="manage_entitylist"
			entity="wfStep" menuAccess="false">
			<property name="searchTable" value="wfStepTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="false" />
			<property name="maintainForm" value="wfStepForm" />
			<property name="baseRestriction" value="interactiveSteps" />
			<filter name="interactiveSteps"
				description="$m{workflow.steps.filter.interactive}">
				<restriction type="equals" field="type" paramA="USR" />
			</filter>
		</applet>
		<applet name="manageWfStepRoles"
			description="$m{workflow.steprole.manage}"
			label="$m{workflow.steprole.manage.label}" type="manage_entitylist_assign"
			entity="wfStepRole" menuAccess="false">
			<property name="searchTable" value="wfStepRoleTable" />
			<property name="searchTable.edit" value="true" />
			<property name="searchTable.report" value="false" />
			<property name="assignmentPage" value="wfStepRoleAssignment" />
		</applet>
		<applet name="manageWorkflowChannel"
			description="$m{workflow.workflowchannel.manage}"
			label="$m{workflow.workflowchannel.manage.label}" icon="project-diagram"
			type="manage_entitylist" entity="workflowChannel" menuAccess="true">
			<property name="searchTable" value="workflowChannelTable" />
			<property name="searchTable.new" value="true" />
			<property name="searchTable.report" value="true" />
			<property name="searchTable.quickFilter" value="true" />
			<property name="createForm" value="workflowChannelForm" />
			<property name="createForm.save" value="true" />
			<property name="createForm.save.next" value="true" />
			<property name="createForm.save.close" value="true" />
			<property name="maintainForm" value="workflowChannelForm" />
			<property name="maintainForm.update" value="true" />
			<property name="maintainForm.delete" value="true" />
			<property name="maintainForm.delete.condition"
				value="delatableWorkflowChannel" />
			<filter name="delatableWorkflowChannel"
				description="$m{workflow.workflowchannel.filter.deletableworkflowchannel}">
				<restriction type="equals" field="configType" paramA="C" />
			</filter>
		</applet>
		<applet name="workflowItem" description="$m{workflow.wfitem.manage}"
			label="$m{workflow.wfitem.manage.label}" icon="list" type="manage_entitylist"
			entity="wfItem" menuAccess="true">
			<property name="searchTable" value="wfItemTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="true" />
			<property name="maintainForm" value="wfItemForm" />
		</applet>
	</applets>
	<!-- References -->
	<references>
		<reference name="workflowRef"
			description="$m{workflow.workflow.reference}" entity="workflow"
			searchField="description" />
		<reference name="workflowFilterRef"
			description="$m{workflow.workflowfilter.reference}"
			entity="workflowFilter" searchField="description" />
		<reference name="wfStepRef"
			description="$m{workflow.step.reference}" entity="wfStep"
			searchField="description" />
		<reference name="wfStepSetValuesRef"
			description="$m{workflow.stepsetvalues.reference}" entity="wfStepSetValues"
			searchField="description" />
		<reference name="wfStepRoutingRef"
			description="$m{workflow.steprouting.reference}" entity="wfStepRouting"
			searchField="description" />
		<reference name="wfStepUserActionRef"
			description="$m{workflow.stepuseraction.reference}" entity="wfStepUserAction"
			searchField="description" />
		<reference name="wfStepAlertRef"
			description="$m{workflow.stepalert.reference}" entity="wfStepAlert"
			searchField="description" />
		<reference name="wfStepRoleRef"
			description="$m{workflow.steprole.reference}" entity="wfStepRole"
			searchField="description" />
		<reference name="wfItemHistRef"
			description="$m{workflow.wfitemhist.reference}" entity="wfItemHist" />
		<reference name="wfItemEventRef"
			description="$m{workflow.wfitemevent.reference}" entity="wfItemEvent" />
		<reference name="wfItemRef"
			description="$m{workflow.wfitem.reference}" entity="wfItem" />
		<reference name="workflowChannelRef"
			description="$m{workflow.workflowchannel.reference}" entity="workflowChannel"
			searchField="description" />
	</references>
	<!-- Entities -->
	<entities>
		<entity name="workflow" description="$m{workflow.workflow}"
			label="$m{workflow.workflow}"
			type="com.flowcentraltech.flowcentral.workflow.entities.Workflow" reportable="true"
			auditable="true">
			<field name="label" label="$m{workflow.workflow.label}" type="string" maxLen="64"  />
			<field name="entity" label="$m{workflow.workflow.entity}" type="string" maxLen="128"  />
			<field name="descFormat"
				label="$m{workflow.workflow.itemdescformat}" type="string"
				nullable="true" />
			<field name="filterList" label="$m{workflow.workflow.filters}"
				type="child_list" references="workflowFilterRef" />
			<field name="stepList" label="$m{workflow.workflow.steps}"
				type="child_list" references="wfStepRef" />
		</entity>
		<entity name="workflowFilter"
			description="$m{workflow.workflowfilter}"
			label="$m{workflow.workflowfilter.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WorkflowFilter">
			<field name="workflowId"
				label="$m{workflow.workflowfilter.workflowid}" type="ref"
				references="workflowRef" inputWidget="application.entitylist" />
			<field name="filter" label="$m{workflow.workflowfilter.filter}"
				type="child" category="workflow" references="application.appFilterRef" />
			<uniqueConstraint name="nameUc"
				description="$m{workflow.workflowfilter.uc.name}"
				fieldList="workflowId,name" />
			<uniqueConstraint name="descUc"
				description="$m{workflow.workflowfilter.uc.description}"
				fieldList="workflowId,description" />
		</entity>
		<entity name="wfStep" description="$m{workflow.step}"
			label="$m{workflow.step}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStep" reportable="false"
			auditable="false">
			<field name="workflowId" label="$m{workflow.step.workflowid}"
				type="ref" references="workflowRef" filterLabel="$m{workflow.step.workflow}"
				inputWidget="application.entitysearch" />
			<field name="type" label="$m{workflow.step.type}" type="enum_ref"
				references="wfsteptypelist" inputWidget="application.enumlist" />
			<field name="priority" label="$m{workflow.step.priority}"
				type="enum_ref" references="wfstepprioritylist"
				inputWidget="application.enumlist" />
			<field name="recordActionType"
				label="$m{workflow.step.recordactiontype}" type="enum_ref"
				references="recordactiontypelist" inputWidget="application.enumlist" />
			<field name="label" label="$m{workflow.step.label}" type="string" maxLen="64"  />
			<field name="formName" label="$m{workflow.step.formname}" type="string"
				nullable="true"  maxLen="128" />
			<field name="nextStepName" label="$m{workflow.step.nextstepname}"
				type="string" nullable="true" />
			<field name="policy" label="$m{workflow.step.policy}" type="string"
				 maxLen="64" nullable="true" inputWidget="wfsteppolicylist" />
			<field name="rule" label="$m{workflow.step.rule}" type="string"
				 maxLen="64" nullable="true" inputWidget="wfsteprulelist" />
			<field name="criticalMinutes" label="$m{workflow.step.criticalminutes}"
				type="integer" />
			<field name="expiryMinutes" label="$m{workflow.step.expiryminutes}"
				type="integer" />
			<field name="audit" label="$m{workflow.step.audit}" type="boolean" />
			<field name="branchOnly" label="$m{workflow.step.branchonly}"
				type="boolean" />
			<field name="includeForwarder"
				label="$m{workflow.step.includeforwarder}" type="boolean" />
			<field name="typeDesc" label="$m{workflow.step.typedesc}"
				type="list_only" key="type" property="description" />
			<field name="priorityDesc" label="$m{workflow.step.prioritydesc}"
				type="list_only" key="priority" property="description" />
			<field name="recordActionTypeDesc"
				label="$m{workflow.step.recordactiontypedesc}" type="list_only"
				key="recordActionType" property="description" />
			<field name="workflowName" label="$m{workflow.step.workflowname}"
				type="list_only" key="workflowId" property="name" />
			<field name="workflowDesc" label="$m{workflow.step.workflowdesc}"
				type="list_only" key="workflowId" property="description" />
			<field name="applicationName" label="$m{workflow.step.applicationname}"
				type="list_only" key="workflowId" property="applicationName" />
			<field name="applicationDesc" label="$m{workflow.step.applicationdesc}"
				type="list_only" key="workflowId" property="applicationDesc" />
			<field name="routingList" label="$m{workflow.step.routings}"
				type="child_list" references="wfStepRoutingRef" />
			<field name="userActionList" label="$m{workflow.step.useractions}"
				type="child_list" references="wfStepUserActionRef" />
			<field name="alertList" label="$m{workflow.step.alerts}"
				type="child_list" references="wfStepAlertRef" />
			<field name="roleList" label="$m{workflow.step.roles}"
				type="child_list" references="wfStepRoleRef" />
		</entity>
		<entity name="wfStepSetValues" description="$m{workflow.stepsetvalues}"
			label="$m{workflow.stepsetvalues.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepSetValues"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.stepsetvalues.stepid}"
				type="ref" references="wfStepRef"
				filterLabel="$m{workflow.stepsetvalues.step}"
				inputWidget="application.entitylist" />
			<field name="wfStepDesc" label="$m{workflow.stepsetvalues.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
			<field name="setValues" label="$m{workflow.stepsetvalues.values}"
				type="child" category="wfstep" references="application.appSetValuesRef" />
		</entity>
		<entity name="wfStepRouting" description="$m{workflow.steprouting}"
			label="$m{workflow.steprouting.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepRouting"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.steprouting.stepid}"
				type="ref" references="wfStepRef"
				filterLabel="$m{workflow.steprouting.step}"
				inputWidget="application.entitylist" />
			<field name="nextStepName"
				label="$m{workflow.steprouting.nextstepname}" type="string"  maxLen="64" />
			<field name="conditionName"
				label="$m{workflow.steprouting.filter}" type="string"  maxLen="64" />
			<field name="wfStepDesc" label="$m{workflow.steprouting.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
		</entity>
		<entity name="wfStepUserAction"
			description="$m{workflow.stepuseraction}"
			label="$m{workflow.stepuseraction.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepUserAction"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.stepuseraction.stepid}"
				type="ref" references="wfStepRef"
				filterLabel="$m{workflow.stepuseraction.step}"
				inputWidget="application.entitylist" />
			<field name="commentRequirement"
				label="$m{workflow.stepuseraction.commentrequirement}" type="enum_ref"
				references="requirementtypelist" inputWidget="application.enumlist" />
			<field name="highlightType"
				label="$m{workflow.stepuseraction.highlighttype}" type="enum_ref"
				references="highlighttypelist" inputWidget="application.enumlist" nullable = "true" />
			<field name="label" label="$m{workflow.stepuseraction.label}"
				type="string" maxLen="96"  />
			<field name="nextStepName"
				label="$m{workflow.stepuseraction.nextstepname}" type="string" maxLen="64"  />
			<field name="orderIndex" label="$m{workflow.stepuseraction.orderindex}"
				type="integer" />
			<field name="validatePage"
				label="$m{workflow.stepuseraction.validatepage}" type="boolean" />
			<field name="wfStepDesc" label="$m{workflow.stepuseraction.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
			<field name="commentRequirementDesc"
				label="$m{workflow.stepuseraction.commentrequirementdesc}" type="list_only"
				key="commentRequirement" property="description" />
			<field name="highlightTypeDesc"
				label="$m{workflow.stepuseraction.highlighttypedesc}" type="list_only"
				key="highlightType" property="description" />
		</entity>
		<entity name="wfStepAlert" description="$m{workflow.stepalert}"
			label="$m{workflow.stepalert.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepAlert" reportable="false"
			auditable="false">
			<field name="wfStepId" label="$m{workflow.stepalert.stepid}" type="ref"
				references="wfStepRef" filterLabel="$m{workflow.stepalert.step}"
				inputWidget="application.entitylist" />
			<field name="type" label="$m{workflow.stepalert.type}" type="enum_ref"
				references="wfalerttypelist" inputWidget="application.enumlist" />
			<field name="notificationType"
				label="$m{workflow.stepalert.notificationtype}" type="enum_ref"
				references="notificationtypelist" inputWidget="application.enumlist" />
			<field name="recipientPolicy"
				label="$m{workflow.stepalert.recipientpolicy}" type="string" maxLen="64"  nullable="true" />
			<field name="recipientNameRule"
				label="$m{workflow.stepalert.recipientnamerule}" type="string" maxLen="64"  nullable="true" />
			<field name="recipientContactRule"
				label="$m{workflow.stepalert.recipientcontactrule}" type="string" maxLen="64"  nullable="true" />
			<field name="template" label="$m{workflow.stepalert.template}"
				type="string"  maxLen="128" />
			<field name="fireOnPrevStepName"
				label="$m{workflow.stepalert.fireonprevstepname}" type="string"
				nullable="true" maxLen="64"  />
			<field name="fireOnConditionName"
				label="$m{workflow.stepalert.fireonfilter}" type="string"
				nullable="true" maxLen="64"  />
			<field name="wfStepDesc" label="$m{workflow.stepalert.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
			<field name="notificationTypeDesc"
				label="$m{workflow.stepalert.notificationtypedesc}" type="list_only"
				key="notificationType" property="description" />
			<field name="typeDesc" label="$m{workflow.stepalert.typedesc}"
				type="list_only" key="type" property="description" />
		</entity>
		<entity name="wfStepRole" description="$m{workflow.steprole}"
			label="$m{workflow.steprole.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepRole" reportable="false"
			auditable="false">
			<field name="wfStepId" label="$m{workflow.steprole.stepid}" type="ref"
				references="wfStepRef" filterLabel="$m{workflow.steprole.step}"
				inputWidget="application.entitylist" />
			<field name="roleId" label="$m{workflow.steprole.roleid}" type="ref"
				references="organization.roleRef" filterLabel="$m{workflow.steprole.role}"
				inputWidget="application.entitysearch" />
			<field name="wfStepName" label="$m{workflow.steprole.stepname}"
				type="list_only" key="wfStepId" property="name" />
			<field name="wfStepDesc" label="$m{workflow.steprole.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
			<field name="workflowName" label="$m{workflow.steprole.workflowname}"
				type="list_only" key="wfStepId" property="workflowName" />
			<field name="workflowDesc" label="$m{workflow.steprole.workflowdesc}"
				type="list_only" key="wfStepId" property="workflowDesc" />
			<field name="applicationName"
				label="$m{workflow.steprole.applicationname}" type="list_only"
				key="wfStepId" property="applicationName" />
			<field name="applicationDesc"
				label="$m{workflow.steprole.applicationdesc}" type="list_only"
				key="wfStepId" property="applicationDesc" />
			<field name="roleCode" label="$m{workflow.steprole.rolecode}"
				type="list_only" key="roleId" property="code" />
			<field name="roleDesc" label="$m{workflow.steprole.roledesc}"
				type="list_only" key="roleId" property="description" />
		</entity>
		<entity name="wfItemHist" description="$m{workflow.wfitemhist}"
			label="$m{workflow.wfitemhist.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfItemHist">
			<field name="applicationName"
				label="$m{workflow.wfitemhist.applicationname}" type="string"  maxLen="64" />
			<field name="workflowName" label="$m{workflow.wfitemhist.workflowname}"
				type="string"  maxLen="128" />
			<field name="entity" label="$m{workflow.wfitemhist.entity}"
				type="string"  maxLen="128" />
			<field name="originWorkRecId"
				label="$m{workflow.wfitemhist.originworkrecid}" type="long" nullable="true" />
			<field name="branchCode" label="$m{workflow.wfitemhist.branchcode}"
				type="string" maxLen="32"  nullable="true" />
			<field name="departmentCode"
				label="$m{workflow.wfitemhist.departmentcode}" type="string" maxLen="32"  nullable="true" />
			<field name="itemDesc" label="$m{workflow.wfitemhist.wfitemdesc}"
				type="string"  maxLen="196" />
			<field name="initiatedBy" label="$m{workflow.wfitemhist.initiatedby}"
				type="string"  maxLen="96" />
		</entity>
		<entity name="wfItemEvent" description="$m{workflow.wfitemevent}"
			label="$m{workflow.wfitemevent.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfItemEvent">
			<field name="wfItemHistId"
				label="$m{workflow.wfitemevent.wfitemhistid}" type="ref"
				references="wfItemHistRef" inputWidget="application.entitylist" />
			<field name="priority" label="$m{workflow.wfitemevent.priority}"
				type="enum_ref" references="wfstepprioritylist"
				inputWidget="application.enumlist" />
			<field name="wfStepName" label="$m{workflow.wfitemevent.wfstepname}"
				type="string" maxLen="64"  />
			<field name="stepDt" label="$m{workflow.wfitemevent.stepdt}"
				type="timestamp_utc" />
			<field name="expectedDt" label="$m{workflow.wfitemevent.expecteddt}"
				type="timestamp_utc" nullable="true" />
			<field name="criticalDt" label="$m{workflow.wfitemevent.criticaldt}"
				type="timestamp_utc" nullable="true" />
			<field name="actionDt" label="$m{workflow.wfitemevent.actiondt}"
				type="timestamp_utc" nullable="true" />
			<field name="actor" label="$m{workflow.wfitemevent.actor}"
				type="string"  maxLen="64" nullable="true" />
			<field name="wfAction" label="$m{workflow.wfitemevent.wfaction}"
				type="string" maxLen="32"  nullable="true" />
			<field name="comment" label="$m{workflow.wfitemevent.comment}"
				type="string" maxLen="512"  nullable="true" />
			<field name="prevWfStepName"
				label="$m{workflow.wfitemevent.prevwfstepname}" type="string"
				 maxLen="64" nullable="true" />
			<field name="errorCode" label="$m{workflow.wfitemevent.errorcode}"
				type="string" maxLen="32"  nullable="true" />
			<field name="errorMsg" label="$m{workflow.wfitemevent.errormsg}"
				type="string" nullable="true" />
			<field name="applicationName"
				label="$m{workflow.wfitemevent.applicationname}" type="list_only"
				key="wfItemHistId" property="applicationName" />
			<field name="workflowName"
				label="$m{workflow.wfitemevent.workflowname}" type="list_only"
				key="wfItemHistId" property="workflowName" />
			<field name="entity" label="$m{workflow.wfitemevent.entity}"
				type="list_only" key="wfItemHistId" property="entity" />
			<field name="originWorkRecId"
				label="$m{workflow.wfitemevent.originworkrecid}" type="list_only"
				key="wfItemHistId" property="originWorkRecId" />
			<field name="wfItemDesc" label="$m{workflow.wfitemevent.wfitemdesc}"
				type="list_only" key="wfItemHistId" property="itemDesc" />
			<field name="branchCode" label="$m{workflow.wfitemevent.branchcode}"
				type="list_only" key="wfItemHistId" property="branchCode" />
			<field name="departmentCode"
				label="$m{workflow.wfitemevent.departmentcode}" type="list_only"
				key="wfItemHistId" property="departmentCode" />
			<field name="priorityDesc"
				label="$m{workflow.wfitemevent.prioritydesc}" type="list_only"
				key="priority" property="description" />
		</entity>
		<entity name="wfItem" description="$m{workflow.wfitem}"
			label="$m{workflow.wfitem.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfItem" reportable="true"
			auditable="true">
			<field name="wfItemEventId" label="$m{workflow.wfitem.wfitemeventid}"
				type="ref" references="wfItemEventRef" inputWidget="application.entitylist" />
			<field name="heldBy" label="$m{workflow.wfitem.heldby}" maxLen="64" nullable="true"  type="string" />
			<field name="forwardedBy" label="$m{workflow.wfitem.forwardedby}"
				type="string" maxLen="64" nullable="true" />
			<field name="criticalAlertSent"
				label="$m{workflow.wfitem.criticalalertsent}" type="boolean" />
			<field name="expirationAlertSent"
				label="$m{workflow.wfitem.expirationalertsent}" type="boolean" />
			<field name="wfStepName" label="$m{workflow.wfitem.stepname}"
				type="list_only" key="wfItemEventId" property="wfStepName" />
			<field name="stepDt" label="$m{workflow.wfitem.stepdt}"
				type="list_only" key="wfItemEventId" property="stepDt" />
			<field name="expectedDt" label="$m{workflow.wfitem.expecteddt}"
				type="list_only" key="wfItemEventId" property="expectedDt" />
			<field name="criticalDt" label="$m{workflow.wfitem.criticaldt}"
				type="list_only" key="wfItemEventId" property="criticalDt" />
			<field name="wfItemDesc" label="$m{workflow.wfitem.itemdesc}"
				type="list_only" key="wfItemEventId" property="wfItemDesc" />
			<field name="branchCode" label="$m{workflow.wfitem.branchcode}"
				type="list_only" key="wfItemEventId" property="branchCode"
				inputWidget="application.entitylist" inputListKey="code"
				references="organization.branchRef" />
			<field name="workflowName" label="$m{workflow.wfitem.workflow}"
				type="list_only" key="wfItemEventId" property="workflowName" />
			<field name="applicationName" label="$m{workflow.wfitem.application}"
				type="list_only" key="wfItemEventId" property="applicationName" />
			<field name="priority" label="$m{workflow.wfitem.priority}"
				type="list_only" key="wfItemEventId" property="priority" />
			<field name="priorityDesc" label="$m{workflow.wfitem.priority}"
				type="list_only" key="wfItemEventId" property="priorityDesc" />
			<field name="errorCode" label="$m{workflow.wfitem.errorcode}"
				type="list_only" key="wfItemEventId" property="errorCode" />
			<field name="errorMsg" label="$m{workflow.wfitem.errormsg}"
				type="list_only" key="wfItemEventId" property="errorMsg" />
