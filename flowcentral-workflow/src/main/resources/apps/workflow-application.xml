		<entity name="wfWizardItem" description="$m{workflow.wfwizarditem}"
			label="$m{workflow.wfwizarditem.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfWizardItem" reportable="true"
			auditable="true">
			<field name="primaryEntityId"
				label="$m{workflow.wfwizarditem.primaryentityid}" type="long" />
			<field name="wizard" label="$m{workflow.wfwizarditem.wizard}"
				type="string" maxLen="128"  />
			<field name="title" label="$m{workflow.wfwizarditem.title}"
				type="string" maxLen="96"  />
			<field name="ownerId" label="$m{workflow.wfwizarditem.ownerid}"
				type="string" maxLen="64"  />
			<field name="percentCompleted"
				label="$m{workflow.wfwizarditem.percentcompleted}" type="integer" />
		</entity>
		<entity name="workflowChannel"
			description="$m{workflow.workflowchannel}"
			label="$m{workflow.workflowchannel}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfChannel" reportable="true"
			auditable="true">
			<field name="direction" label="$m{workflow.workflowchannel.direction}"
				type="enum_ref" references="channeldirectiontypelist"
				inputWidget="application.enumlist" />
			<field name="status" label="$m{workflow.workflowchannel.status}"
				type="enum_ref" references="wfchannelstatuslist"
				inputWidget="application.enumlist" />
			<field name="label" label="$m{workflow.workflowchannel.label}"
				type="string" inputWidget="application.text"  maxLen="64" />
			<field name="entity" label="$m{workflow.workflowchannel.entity}"
				type="string"  maxLen="128" />
			<field name="destination"
				label="$m{workflow.workflowchannel.destination}" type="string"  maxLen="128" />
			<field name="rule" label="$m{workflow.workflowchannel.rule}"
				type="string" nullable="true" />
			<field name="directionDesc"
				label="$m{workflow.workflowchannel.directiondesc}" type="list_only"
				key="direction" property="description" />
			<field name="statusDesc"
				label="$m{workflow.workflowchannel.statusdesc}" type="list_only"
				key="status" property="description" />
		</entity>
	</entities>
	<!-- Tables -->
	<tables>
		<table name="workflowTable" description="$m{workflow.workflow.table}"
			label="$m{workflow.workflow.table.label}" entity="workflow" itemsPerPage="20">
			<column field="applicationDesc" renderWidget="application.text"
				label="$m{workflow.workflow.application}" widthRatio="2" />
			<column field="name" renderWidget="application.text"
				widthRatio="2" linkAct="maintainAct" />
			<column field="description" renderWidget="application.text"
				widthRatio="2" linkAct="maintainAct" />
			<column field="entity" renderWidget="application.text"
				widthRatio="2" />
			<column field="createDt" renderWidget="application.datetime"
				widthRatio="1" />
			<column field="updateDt" renderWidget="application.datetime"
				widthRatio="1" />
		</table>
		<table name="wfStepTable" description="$m{workflow.step.table}"
			label="$m{workflow.step.table.label}" entity="wfStep" itemsPerPage="20">
			<column field="workflowDesc" renderWidget="application.text"
				label="$m{workflow.step.workflow}" widthRatio="1" />
			<column field="name" renderWidget="application.text"
				widthRatio="1" linkAct="maintainAct" />
			<column field="description" renderWidget="application.text"
				widthRatio="1" linkAct="maintainAct" />
			<column field="typeDesc" renderWidget="application.text"
				label="$m{workflow.step.type}" widthRatio="1" />
			<column field="priorityDesc" renderWidget="application.text"
				label="$m{workflow.step.priority}" widthRatio="1" />
		</table>
		<table name="wfStepRoleTable" description="$m{workflow.steprole.table}"
			label="$m{workflow.steprole.table.label}" entity="wfStepRole"
			itemsPerPage="20">
			<column field="wfStepDesc" renderWidget="application.text"
				label="$m{workflow.steprole.step}" widthRatio="1" />
			<column field="roleDesc" renderWidget="application.text"
				label="$m{workflow.steprole.role}" widthRatio="1" />
			<column field="roleCode" renderWidget="application.text"
				widthRatio="1" />
			<column field="updatedBy" renderWidget="application.text"
				label="$m{workflow.steprole.assignedby}" widthRatio="1" />
			<column field="updateDt" renderWidget="application.datetime"
				label="$m{workflow.steprole.assignedon}" widthRatio="1" />
		</table>
		<table name="wfItemTable" description="$m{workflow.wfitem.table}"
			label="$m{workflow.wfitem.table.label}" entity="wfItem" itemsPerPage="20">
			<column field="applicationName" renderWidget="application.text"
				widthRatio="2" />
			<column field="workflowName" renderWidget="application.text"
				widthRatio="2" />
			<column field="wfItemDesc" renderWidget="application.text"
				widthRatio="2" linkAct="maintainAct" />
			<column field="wfStepName" renderWidget="application.text"
				widthRatio="2" />
			<column field="stepDt" renderWidget="application.datetime"
				widthRatio="2" />
			<column field="forwardedBy" renderWidget="application.text"
				widthRatio="2" />
			<column field="heldBy" renderWidget="application.text"
				widthRatio="2" />
			<column field="priorityDesc" renderWidget="application.text"
				widthRatio="2" />
		</table>
		<table name="wfItemReviewTable"
			description="$m{workflow.wfitem.review.table}"
			label="$m{workflow.wfitem.review.table.label}" entity="wfItem"
			itemsPerPage="20">
			<column field="wfItemDesc" renderWidget="application.text"
				widthRatio="2" linkAct="decisionAct" />
			<column field="wfStepName" renderWidget="application.text"
				widthRatio="2" />
			<column field="stepDt" renderWidget="application.datetime"
				widthRatio="2" />
			<column field="forwardedBy" renderWidget="application.text"
				widthRatio="2" />
			<column field="heldBy" renderWidget="application.text"
				widthRatio="2" />
			<column field="priorityDesc" renderWidget="application.text"
				widthRatio="2" />
		</table>
		<table name="wfWizardItemReviewTable"
			description="$m{workflow.wfwizarditem.review.table}"
			label="$m{workflow.wfwizarditem.review.table.label}" entity="wfWizardItem"
			itemsPerPage="20">
			<column field="wizard" renderWidget="application.text"
				widthRatio="2" />
			<column field="title" renderWidget="application.text"
				widthRatio="3" linkAct="decisionAct" />
			<column field="percentCompleted"
				renderWidget="application.progressbar" widthRatio="2" />
			<column field="ownerId" renderWidget="application.text"
				widthRatio="2" />
			<column field="createDt" renderWidget="application.datetime"
				widthRatio="2" />
			<column field="updateDt" renderWidget="application.datetime"
				widthRatio="2" />
		</table>
		<table name="workflowChannelTable"
			description="$m{workflow.workflowchannel.table}"
			label="$m{workflow.workflowchannel.table.label}" entity="workflowChannel"
			itemsPerPage="20">
			<column field="applicationDesc" renderWidget="application.text"
				label="$m{workflow.workflowchannel.application}" widthRatio="1" />
			<column field="name" renderWidget="application.text"
				widthRatio="1" linkAct="maintainAct" />
			<column field="description" renderWidget="application.text"
				widthRatio="1" linkAct="maintainAct" />
			<column field="entity" renderWidget="application.text"
				widthRatio="1" />
			<column field="directionDesc" renderWidget="application.text"
				widthRatio="1" />
			<column field="statusDesc" renderWidget="application.text"
				widthRatio="1" />
			<column field="createDt" renderWidget="application.datetime"
				widthRatio="1" />
			<column field="updateDt" renderWidget="application.datetime"
				widthRatio="1" />
		</table>
	</tables>
	<!-- Forms -->
	<forms>
		<form name="workflowForm" description="$m{workflow.workflow.form}"
			entity="workflow">
			<tab name="basicDetails" label="$m{workflow.workflow.form.basicdetails}"
				contentType="miniform" editable="false">
				<section name="workflowDetails" columns="type_2">
					<field name="applicationId" inputWidget="application.entitylist"
						column="0" label="$m{workflow.workflow.application}" required="true" />
					<field name="name" inputWidget="application.name" column="0"
						required="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="entity" inputWidget="application.appworkappentitylist"
						column="1" required="true" />
				</section>
			</tab>
			<tab name="interactiveSteps"
				label="$m{workflow.workflow.form.interactivesteps}" contentType="child_list"
				applet="manageInteractiveSteps" reference="stepList" />
		</form>
		<form name="wfStepForm" description="$m{workflow.step.form}"
			entity="wfStep">
			<tab name="basicDetails" label="$m{workflow.step.form.basicdetails}"
				contentType="miniform" editable="false">
				<section name="stepDetails" columns="type_2">
					<field name="workflowId" inputWidget="application.entitylist"
						column="0" label="$m{workflow.step.workflow}" required="true" />
					<field name="name" inputWidget="application.name" column="0"
						required="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="formName" inputWidget="application.text" column="0" />
					<field name="type" inputWidget="application.enumlist" column="1"
						required="true" />
					<field name="recordActionType" inputWidget="application.enumlist"
						column="1" required="true" visible="false" />
					<field name="policy" column="1" required="true" visible="false" />
					<field name="rule" column="1" visible="false" />
					<field name="priority" inputWidget="application.enumlist" column="1"
						required="true" />
					<field name="criticalMinutes" inputWidget="application.integer"
						column="1" />
					<field name="expiryMinutes" inputWidget="application.integer"
						column="1" />
					<field name="audit" inputWidget="application.checkbox" column="1" />
					<field name="branchOnly" inputWidget="application.checkbox"
						column="1" />
					<field name="includeForwarder" inputWidget="application.checkbox"
						column="1" />
				</section>
			</tab>
			<tab name="assignedRoles" label="$m{workflow.step.form.assignedroles}"
				contentType="child_list" applet="manageWfStepRoles" reference="roleList" />
			<formStatePolicy type="on_switch" name="policyPolicy"
				description="$m{workflow.step.form.fieldstatepolicy.policy}">
				<onCondition>
					<restriction type="amongst" field="type" paramA="N|P" />
				</onCondition>
				<setStates>
					<setState type="field" target="policy,rule" visible="true" />
				</setStates>
			</formStatePolicy>
			<formStatePolicy type="on_switch" name="recordActionPolicy"
				description="$m{workflow.step.form.fieldstatepolicy.recordactiontype}">
				<onCondition>
					<restriction type="equals" field="type" paramA="R" />
				</onCondition>
				<setStates>
					<setState type="field" target="recordActionType" visible="true" />
				</setStates>
			</formStatePolicy>
		</form>
		<form name="wfItemForm" description="$m{workflow.wfitem.form}"
			entity="wfItem">
			<tab name="basicDetails" label="$m{workflow.wfitem.form.basicdetails}"
				contentType="miniform" editable="false">
				<section name="wfItemDetails" columns="type_2">
					<field name="applicationName" inputWidget="application.text"
						column="0" required="true" />
					<field name="workflowName" inputWidget="application.text"
						column="0" required="true" />
					<field name="wfStepName" inputWidget="application.text"
						column="0" required="true" />
					<field name="wfItemDesc" inputWidget="application.text"
						column="0" required="true" />
					<field name="forwardedBy" inputWidget="application.text"
						column="0" required="true" />
					<field name="heldBy" inputWidget="application.text" column="0" />
					<field name="priorityDesc" inputWidget="application.text"
						column="1" required="true" />
					<field name="stepDt" inputWidget="application.datetime" column="1"
						required="true" />
					<field name="criticalDt" inputWidget="application.datetime"
						column="1" />
					<field name="expectedDt" inputWidget="application.datetime"
						column="1" />
					<field name="criticalAlertSent" inputWidget="application.checkbox"
						column="1" />
					<field name="expirationAlertSent" inputWidget="application.checkbox"
						column="1" />
				</section>
				<section name="errorDetails" columns="type_1">
					<field name="errorCode" inputWidget="application.text"
						column="0" />
					<field name="errorMsg" inputWidget="application.textarea"
						column="0" />
				</section>
			</tab>
		</form>
		<form name="workflowChannelForm"
			description="$m{workflow.workflowchannel.form}" entity="workflowChannel">
			<tab name="basicDetails"
				label="$m{workflow.workflowchannel.form.basicdetails}"
				contentType="miniform">
				<section name="workflowChannelDetails" columns="type_2">
					<field name="applicationId" inputWidget="application.entitylist"
						column="0" label="$m{workflow.workflowchannel.application}"
						switchOnChange="true" required="true" disabled="false" />
					<field name="name" inputWidget="application.name" column="0"
						required="true" disabled="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" disabled="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="direction" inputWidget="application.enumlist" column="1"
						required="true" disabled="true" />
					<field name="entity" inputWidget="application.appworkappentitylist"
						column="1" required="true" disabled="true" switchOnChange="true" />
					<field name="destination" inputWidget="wfchanneldestinationlist"
						column="1" required="true" />
					<field name="rule" inputWidget="application.text" column="1" />
					<field name="status" inputWidget="application.enumlist" column="1"
						required="true" />
				</section>
			</tab>
			<tab name="changeLog" label="$m{application.form.audit}"
				contentType="miniform_changelog" />
			<formStatePolicy type="on_switch" name="editPolicy"
				description="$m{workflow.workflowchannel.form.fieldstatepolicy.editpolicy}">
				<onCondition>
					<restriction type="equals" field="configType" paramA="C" />
				</onCondition>
				<setStates>
					<setState type="field" target="applicationId,name,description,direction,entity" disabled="false" />
				</setStates>
			</formStatePolicy>
		</form>
	</forms>
	<!-- Assignment Pages -->
	<assignmentPages>
		<assignmentPage name="wfStepRoleAssignment"
			description="$m{workflow.step.role.assignment}"
			label="$m{workflow.step.role.assignment}" entity="wfStepRole"
			baseField="wfStepId" assignField="roleId"
			assignCaption="$m{workflow.step.role.assigned}"
			unassignCaption="$m{workflow.step.role.available}" assignList="roleinlist"
			unassignList="rolenotinlist" />
	</assignmentPages>
</application>